<title>History - <%= user.email %>
</title>
<section class="history-page sections">
    <div class="rap">
        <div class="container">
            <a class="btn btn-outline-info" href="/account/<%= user._id %>"><i class="bi bi-arrow-left"></i> Back</a>
        </div>
        <div class="videos container mt-4">
            <% for (let key in historyVid) { %>
            <a href="">
                <img src="/videos/img/<%= historyVid[key].img %>" alt="img">
                <div>
                    <h5>Lorem Ipsum is simply dummy text of the printing and typesetting </h5>
                    <span><%= historyVid[key].views %> views</span> |
                    <span><%= historyVid[key].category %></span>
                    <p><%= historyVid[key].description %></p>
                </div>
            </a>
            <% } %>
        </div>
    </div>
</section>