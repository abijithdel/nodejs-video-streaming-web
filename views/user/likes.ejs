<title>Likes - <%= user.email %></title>
<section class="likes-page">
    <div class="rap">
        <div class="container">
            <a class="btn btn-outline-info" href="/account/<%= user._id %>"><i class="bi bi-arrow-left"></i> Back</a>
        </div>
        <div class="videos container mt-3">
            <% for (let key in likesVid) { %>
            <a href="">
                <img src="/videos/img/<%= likesVid[key].img %>" alt="img">
                <h5><%= likesVid[key].title %></h5>
                <div>
                    <span><%= likesVid[key].views %> Views</span> | 
                    <span><%= likesVid[key].category %></span>
                    <p><%= likesVid[key].description %></p>
                </div>
            </a>
            <% } %>
        </div>
    </div>
</section>