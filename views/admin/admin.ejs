        <div class="admin-menu">
            <a title="new video" href="/" class="mt-2 text-danger"><i class="bi bi-arrow-left"></i> <span>Back</span></a>
            <a title="new video" href="/admin/new-video" class="mt-2"><i class="bi bi-film"></i> <span>New Video</span></a>
            <a title="all users" href="/admin/all-users" class="mt-3"><i class="bi bi-people-fill"></i> <span>All Users</span></a>
