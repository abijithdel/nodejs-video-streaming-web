        <div class="admin-panel">
            <div class="container">
                <div>
                    <h5>Videos</h5>
                    <span><%= videoleng %></span>
                </div>
                <div>
                    <h5>Users</h5>
                    <span><%= usersleng %></span>
                </div>
                <div>
                    <h5>Likes</h5>
                    <span><%= likesleng %></span>
